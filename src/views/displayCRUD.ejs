<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Table</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 20px;
        }
        th, td {
            padding: 12px;
            border: 1px solid #ddd;
            text-align: left;
        }
        th {
            background-color: #f4f4f4;
        }
        tr:nth-child(even) {
            background-color: #f9f9f9;
        }
        tr:hover {
            background-color: greenyellow;
        }
        .container {
            max-width: 1200px;
            margin: auto;
        }
        h1 {
            text-align: center;
            margin-bottom: 20px;
        }
    </style>
</head>
<body>
    <div class="container">
        <h1>User Table</h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Email</th>
                    <th>Password</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Address</th>
                    <th>Phone Number</th>
                    <th>Gender</th>
                    <th>Image</th>
                    <th>Role ID</th>
                    <th>Position ID</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                </tr>
            </thead>
            <tbody>
                <% dataTable.forEach(function(user) { %>
                    <tr>
                        <td><%= user.id %></td>
                        <td><%= user.email %></td>
                        <td>********</td> <!-- Assuming you don't want to display the actual password -->
                        <td><%= user.firstName %></td>
                        <td><%= user.lastName %></td>
                        <td><%= user.address %></td>
                        <td><%= user.phonenumber %></td>
                        <td><%= user.gender %></td>
                        <td><%= user.image %></td>
                        <td><%= user.roleId %></td>
                        <td><%= user.positionId %></td>
                        <td><%= user.createdAt %></td>
                        <td><%= user.updatedAt %></td>
                    </tr>
                    <% }); %>
                <!-- Example row -->
                <tr>
                    <td>1</td>
                    <td>example@example.com</td>
                    <td>********</td>
                    <td>John</td>
                    <td>Doe</td>
                    <td>123 Main St</td>
                    <td>123-456-7890</td>
                    <td>Male</td>
                    <td>image.jpg</td>
                    <td>admin</td>
                    <td>manager</td>
                    <td>2023-01-01</td>
                    <td>2023-01-02</td>
                </tr>
                <!-- Add more rows as needed -->
            </tbody>
        </table>
    </div>
</body>
</html>